        xmlns:local="clr-namespace:Labb_3" 
        xmlns:view="clr-namespace:Labb_3.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        MinHeight="500" MinWidth="650">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
