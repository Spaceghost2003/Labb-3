             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp" 
             xmlns:local="clr-namespace:Labb_3.View"
             DataContext="{Binding PlayerViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button  HorizontalAlignment="Left" Margin="262,132,0,0" VerticalAlignment="Top" Height="75" Width="110" Command="{Binding UpdateButtonCommand}"  Content="{Binding Path=TestData}"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="396,132,0,0" VerticalAlignment="Top" Height="75" Width="110"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="262,224,0,0" VerticalAlignment="Top" Height="75" Width="110"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="396,224,0,0" VerticalAlignment="Top" Height="75" Width="110"/>


    </Grid>

</UserControl>
